#!/usr/bin/env python

"""
setup.py file for Siconos 
"""

from distutils.core import setup, Extension


Siconos_module = Extension('_Siconos',
                           sources=[@SICONOS_SWIG_SRCS@],
                           )

setup (name = 'SiconosNumerics',
       version = '0.1',
       author      = "Siconos Developement team",
       description = """Siconos Numerics""",
       ext_modules = [Siconos_module],
       py_modules = ["SiconosNumerics"],
       )
