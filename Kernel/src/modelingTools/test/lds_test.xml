<!-- SICONOS Model XML file -->

<SiconosModel>
	<NSDS bvp='false'>
	<!-- DSs defined in the problem -->
		<DS_Definition>		
			<FirstOrderLinearDS number='13'  Id="testDS1">
	 			<StepsInMemory>2</StepsInMemory>				
				<x0 vectorSize='3'>
					1.0 2.0 3.0
				</x0>
				<b vectorSize='3'>
					4.0 5.0 6.0
				</b>
				<A matrixRowSize='3' matrixColSize='3'>	
					<row>2.0 2.0 2.0</row>
					<row>4.0 4.0 4.0</row>
					<row>6.0 6.0 6.0</row>
				</A>			
			</FirstOrderLinearDS>
			<FirstOrderLinearDS number='2' Id="testDS2">
				<x0 vectorSize='3'>
					2.0 4.0 6.0
				</x0>
				<b vectorPlugin="TestPlugin:computeB"/>
				<A matrixPlugin="TestPlugin:computeA"/>	
 			</FirstOrderLinearDS>
		</DS_Definition>
	</NSDS>
</SiconosModel>
