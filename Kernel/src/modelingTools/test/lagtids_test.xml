<!-- SICONOS Model XML file -->

<SiconosModel>
	<NSDS bvp='false'>
	<!-- DSs defined in the problem -->
		<DS_Definition>		
			<LagrangianLinearTIDS number='13' Id="testLAGTIDS1">
				<StepsInMemory>2</StepsInMemory>				
				<q0 vectorSize='3'>
					1.0 2.0 3.0
				</q0>
				<Velocity0  vectorSize='3'>
					4.0 5.0 6.0
				</Velocity0>
				<Mass matrixRowSize='3' matrixColSize='3'>	
					<row>1.0 0.0 0.0</row>
					<row>0.0 2.0 0.0</row>
					<row>0.0 0.0 3.0</row>
				</Mass>			
				<K matrixRowSize='3' matrixColSize='3'>	
					<row>1.0 2.0 3.0</row>
					<row>4.0 5.0 6.0</row>
					<row>7.0 8.0 9.0</row>
				</K>			
				<C matrixRowSize='3' matrixColSize='3'>	
					<row>5.0 6.0 7.0</row>
					<row>2.0 3.0 1.0</row>
					<row>8.0 8.0 9.0</row>
				</C>			
				<FExt vectorPlugin="TestPlugin:computeFExt"/>
			</LagrangianLinearTIDS>
		</DS_Definition>
	</NSDS>
</SiconosModel>
