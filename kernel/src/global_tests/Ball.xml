<!-- SICONOS Model XML file -->

<SiconosModel>
<Title>title</Title>
<Author>author</Author>
<Description>description</Description>
<Date>date</Date>
<SchemaXML>../../config/xmlschema/SiconosModelSchema-V1.2.xsd</SchemaXML>
<Time>	<t0>0.0</t0><T>10</T> </Time>
 <NSDS bvp='false'>
 <!-- DSs defined in the problem -->
 <DS_Definition>		
	<LagrangianLinearTIDS number='1'>
	<Id> Ball </Id>				
	<StepsInMemory>1</StepsInMemory>				
	<q0 vectorSize='3'>1.0 0.0 0.0</q0>
	<Velocity0  vectorSize='3'>0.0 0.0 0.0</Velocity0>
        <FExt vectorPlugin="gTestPlugin:ballFExt"/>
	<Mass matrixRowSize='3' matrixColSize='3'>	
	  <row>1.0 0.0 0.0</row>
	  <row>0.0 1.0 0.0</row>
	  <row>0.0 0.0 1.0</row>
	</Mass>			
	</LagrangianLinearTIDS>
 </DS_Definition>
 <!-- Interactions defined in the problem -->		
 <Interaction_Definition>
 <!-- A definition of a DS interaction, and list of couple of DSs who are concerned by it -->			
 <Interaction number='1' Id='Ball-Ground'>
 <size> 1 </size>
 <DS_Concerned vectorSize='1'>1</DS_Concerned>
 <Interaction_Content>		
 <LagrangianRelation type="LinearTI">
	<C matrixRowSize='1' matrixColSize='3'>
          <row> 1.0  0.0  0.0</row>
	</C>              
 </LagrangianRelation>			
 <!-- NS Law of this interaction	-->
 <NewtonImpactLaw>
  <size>1</size>
  <e>0.9</e>				
 </NewtonImpactLaw>
 </Interaction_Content>	
 </Interaction>				
 </Interaction_Definition>		
</NSDS>
<Simulation type='TimeStepping'>
<TimeDiscretisation isConstant='true'><h>0.005</h></TimeDiscretisation>				
<!-- One Step Integrators -->				
<OneStepIntegrator_Definition>
<!-- A definition of a OneStepIntegrator, and list of couple of DSs who are concerned by it -->			
<Moreau>
<DS_Concerned vectorSize='1'>1</DS_Concerned>		
<Theta all="0.5"></Theta>
</Moreau>					
</OneStepIntegrator_Definition>
<!-- One Step NS Problem of th Simulation-->
<OneStepNSProblems_List>
<LCP StorageType="0" Id="myLCP">
<NonSmoothSolver Name="Lemke">
 <iparam  vectorSize='5'>1000 0 0 0 0</iparam>
 <dparam  vectorSize='5'>1e-12 0 0 0 0</dparam>
</NonSmoothSolver>
</LCP>
</OneStepNSProblems_List>
</Simulation>	
</SiconosModel>
